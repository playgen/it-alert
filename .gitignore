## Ignore Visual Studio temporary files, build results, and
## files generated by popular Visual Studio add-ons.

# Build results
[Dd]ebug/
[Dd]ebugPublic/
[Rr]elease/
[Rr]eleases/
[Xx]64/
[Xx]86/
[Bb]in/
[Oo]bj/

# visual studio auto generated
project.lock.json
artifacts/
*.suo
*.user
*.userosscache
*.sln.docstates
*.vs

# Visual Studio cache files
# files ending in .cache can be ignored
*.[Cc]ache
# but keep track of directories ending in .cache
!*.[Cc]ache/

# ReSharper is a .NET coding add-in
_ReSharper*/	
*.[Rr]e[Ss]harper
*.DotSettings.user

# NuGet Packages
*.nupkg
# The packages folder can be ignored because of Package Restore
**/packages/
*.nuget.props
*.nuget.targets

*.log
*.mdb

## project specific

# unity stuff
Unity/[Ll]ibrary/
Unity/[Tt]emp/
Unity/[Oo]bj/
Unity/[Bb]uild[s]/
Unity/**/*.mdb

# Autogenerated VS/MD/Consulo solution and project files
Unity/*.suo
Unity/*.user
Unity/*.sln
Unity/*.csproj

# we want photon server lib dlls
!/Server/lib/*
# ignore photon server logs
Server/**/log

# Photon
Server/deploy/bin_Win64/PhotonServer.config
Server/Plugins/lib/PhotonHivePlugin.dll
Server/deploy/*/SUGARConfiguration.json
Server/deploy/LoadBalancing/bin/Cache/
!Server/nuget/*

# Exclude dlls, debug symbols and documentation unless otherwise specified
**/*.dll
**/*.pdb
bin/*.xml

!lib/*.dll
!lib/*.pdb
!lib/**/*.dll
!lib/**/*.pdb

lib/**/*.mdb

!lib/**/[Bb]in

# ignore unity assets
Unity/Assets/SUGAR/
Unity/Assets/GameWork/
Unity/Assets/PlayGen.ITAlert.Unity/
Unity/Assets/PlayGen.Photon.Unity/

!Server/deploy/Policy/
!Server/deploy/bin*/*

# exclude plugin - this gets built by plugin project
